{
  "name": "munch-web",
  "version": "1.0.0",
  "description": "A web app RSS feed reader",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:dev && webpack-dev-server",
    "build": "npm run clean && webpack -p && cp src/index.html dist/index.html",
    "build-dev": "npm run clean && webpack -d && cp src/index.html dist/index.html",
    "clean": "rm -rf dist",
    "test-acceptance": "npm run build && wdio"
  },
